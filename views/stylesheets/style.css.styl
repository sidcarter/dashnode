fonts()
  font arguments "Helvetica Neue", Helvetica

border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  border-radius arguments

body
  color #ffffee
  background #000011
  fonts 13px

.container
  width 600px
  padding 10px
  margin 20px
  background #555555
  color #eeffee
  border-radius 5px